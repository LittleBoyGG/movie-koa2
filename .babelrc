{
    "presets": ["env","stage-0","react"],
    "plugins": [
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ],
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-es2015-classes",
    ]
}